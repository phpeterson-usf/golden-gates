{
  "name": "golden-gates-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .js,.vue,.ts",
    "lint:fix": "eslint . --ext .js,.vue,.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "primeicons": "^6.0.1",
    "primevue": "^3.40.0",
    "pyodide": "^0.27.6",
    "vue": "^3.3.4",
    "vue-i18n": "^9.14.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-static-copy": "^3.1.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  }
}
